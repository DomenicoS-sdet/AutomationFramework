  <data name="header" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head&gt;
	&lt;title&gt;Automation Framework Report&lt;/title&gt;
&lt;/head&gt;</value>
  </data>
  <data name="scripts" xml:space="preserve">
    <value>&lt;script&gt;

function searchFeature() {
  // Declare variables 
  var input, filter, table, tr, td, i, txtValue;
  input = document.getElementById("featureSearch");
  filter = input.value.toUpperCase();
  table = document.getElementById("testTable");
  tr = table.getElementsByTagName("tr");

  // Loop through all table rows, and hide those who don't match the search query
  for (i = 0; i &lt; tr.length; i++) {
    td = tr[i].getElementsByClassName("feature")[0];
    if (td) {
      txtValue = td.textContent || td.innerText;
      if (txtValue.toUpperCase().indexOf(filter) &gt; -1) {
        tr[i].style.display = "";
      } else {
        tr[i].style.display = "none";
      }
    } 
  }
}

function searchScenario() {
  // Declare variables 
  var input, filter, table, tr, td, i, txtValue;
  input = document.getElementById("scenarioSearch");
  filter = input.value.toUpperCase();
  table = document.getElementById("testTable");
  tr = table.getElementsByTagName("tr");

  // Loop through all table rows, and hide those who don't match the search query
  for (i = 0; i &lt; tr.length; i++) {
    td = tr[i].getElementsByClassName("scenario")[0];
    if (td) {
      txtValue = td.textContent || td.innerText;
      if (txtValue.toUpperCase().indexOf(filter) &gt; -1) {
        tr[i].style.display = "";
      } else {
        tr[i].style.display = "none";
      }
    } 
  }
}

function searchResult() {
  // Declare variables 
  var input, filter, table, tr, td, i, txtValue;
  input = document.getElementById("resultSearch");
  filter = input.value.toUpperCase();
  table = document.getElementById("testTable");
  tr = table.getElementsByTagName("tr");

  // Loop through all table rows, and hide those who don't match the search query
  for (i = 0; i &lt; tr.length; i++) {
    td = tr[i].getElementsByClassName("result")[0];
    if (td) {
      txtValue = td.textContent || td.innerText;
      if (txtValue.toUpperCase().indexOf(filter) &gt; -1) {
        tr[i].style.display = "";
      } else {
        tr[i].style.display = "none";
      }
    } 
  }
}
&lt;/script&gt;</value>
