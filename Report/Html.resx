  <data name="SearchInput" xml:space="preserve">
    <value>&lt;br&gt;
&lt;table id="searchInput"&gt;
&lt;tr&gt;
&lt;td&gt;&lt;input type="text" id="featureSearch" onkeyup="searchFeature()" placeholder="Search for Feature..."&gt;&lt;/td&gt;
&lt;td&gt;&lt;input type="text" id="scenarioSearch" onkeyup="searchScenario()" placeholder="Search for Scenario..."&gt;&lt;/td&gt;
&lt;td&gt;&lt;input type="text" id="resultSearch" onkeyup="searchResult()" placeholder="Search for Result..."&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;/table&gt;
