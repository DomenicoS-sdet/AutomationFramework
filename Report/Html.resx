  <data name="CSS" xml:space="preserve">
    <value>&lt;style&gt;
				
.datatable {
border: 1px solid black;
}
#overallExecution ,#overallExecution th,#overallExecution td {
	border: 1px solid black;
}

#testTable, #searchInput {
  border-collapse: collapse; /* Collapse borders */
  width: 100%; /* Full-width */
  border: 1px solid #ddd; /* Add a grey border */
  font-size: 18px; /* Increase font-size */
}

#testTable th, #testTable td, #searchInput th, #searchInput td {
  text-align: left; /* Left-align text */
  padding: 12px; /* Add padding */
}

#testTable tr, #searchInput tr {
  /* Add a bottom border to all table rows */
  border-bottom: 1px solid #ddd; 
}

#testTable tr.header, #testTable tr:hover {
  /* Add a grey background color to the table header and on hover */
  background-color: #f1f1f1;
}
				&lt;/style&gt;</value>
